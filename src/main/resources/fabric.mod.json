{
  "schemaVersion": 1,
  "id": "custom-map-resetter",
  "version": "${version}",
  "name": "Custom Map Resetter",
  "description": "Resets custom maps by making copies of a main world and tracking attempts. Based on atum.",
  "authors": [
    "tildejustin"
  ],
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "main": [
      "xyz.tildejustin.custommapresetter.CustomMapResetter"
    ]
  },
  "mixins": [
    "custom-map-resetter.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.12",
    "minecraft": "1.8.9"
  },
  "breaks": {
    "atum": "*"
  }
}
